# Configurações do Servidor
PORT=3000
NODE_ENV=production

# Banco de Dados
DB_PATH=./data/cloaker.db

# Chaves de Segurança (GERE SUAS PRÓPRIAS CHAVES!)
JWT_SECRET=sua_chave_jwt_super_secreta_aqui
SESSION_SECRET=sua_chave_sessao_super_secreta_aqui
ENCRYPTION_KEY=sua_chave_criptografia_32_caracteres_aqui

# Configurações do Cloaker
CLOAKER_MODE=production
DEFAULT_LANDING_PAGE=https://seusite.com
SAFE_PAGE=https://seusite.com/safe

# APIs Externas
IP_API_KEY=sua_chave_api_ip_aqui
USER_AGENT_API_URL=https://api.useragentapi.com

# Logs
LOG_LEVEL=info
LOG_FILE=./logs/cloaker.log

# Rate Limiting
RATE_LIMIT_WINDOW=900000
RATE_LIMIT_MAX=100

# Proxies (opcional)
USE_PROXY=false
PROXY_LIST=

# Recursos Avançados
CAPTCHA_ENABLED=false
HONEYPOT_ENABLED=true
